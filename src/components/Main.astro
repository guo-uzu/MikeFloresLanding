---
import { Image } from "astro:assets"
import HeroSection from "./HeroSection.astro"
import Carousel from "./Carousel.tsx"
import Spot from "./Spot.astro"
import CarouselTrabajo from "./CarouselTrabajo.tsx"
import Todos from "@assets/Todos.webp"
import Columna from "./Columna.tsx"
import Contacto from "./Contacto.tsx"
import Footer from "./Footer.astro"
---
<main>
    <HeroSection />
    <Carousel client:only="react"/>
    <Spot />
    <CarouselTrabajo client:only="react"/>
    <Image src={Todos} alt="" />
    <Columna client:only="react"/>
    <Contacto client:only="react"/>
    <Footer/>
</main>
